<%= turbo_frame_tag "new_ticket_form" do %>
  <%= form_with(model: @ticket, local: true) do |form| %>
    <div class="form-group">
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>
    <div class="form-group">
      <%= form.label :description %>
      <%= form.text_area :description %>
    </div>
    <%= form.submit "Create Ticket" %>
  <% end %>
<% end %>